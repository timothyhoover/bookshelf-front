{
	"include": ["./src/*"],
	"compilerOptions": {
		"lib": ["dom", "es2015"],
		"jsx": "react",
		"esModuleInterop": true,
		"target": "es5",
		"types": ["cypress", "@testing-library/cypress"],
		"allowJs": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	}
}
